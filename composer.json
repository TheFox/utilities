{
    "name": "thefox/utilities",
    "description": "Collection of usefull PHP utilities.",
    "type": "library",
    "keywords": [
        "utilities",
        "bin",
        "binary",
        "hex",
        "hexadecimal",
        "rand",
        "random",
        "debug",
        "leb128",
        "little endian",
        "big endian",
        "endian"
    ],
    "homepage": "https://fox21.at",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Mayer",
            "email": "christian@fox21.at",
            "homepage": "https://fox21.at"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "phpstan/phpstan": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "TheFox\\Utilities\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheFox\\Test\\": ["tests"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x.x-dev"
        }
    }
}
